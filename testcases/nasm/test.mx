int worka(int x, int y) {
	return x + x - y;
}

int workb(int x, int y) {
	return -x + y + y;
}

int workc(int x, int y) {
	return x + x + x - y - y;
}

int main() {
	int a = getInt();
	int b = 0;
	int c = 1;
	int d = 0;
	int e = 1;
	int f = 0;
	int g = 1;
	int i = 0;
	for(i = 1; i <= 10; i++) {
		println(toString(i) + " " + toString(g));
        a = worka(a, b);
		b = workb(a, b);
		c = workc(a, b);
        println(toString(a)+" "+toString(b)+" "+toString(c));
		d = -a + b - c + d + e - f + g;
		e = a + b + c - d - e - f + g;
		f = a + b - c + d - e + f - g;
		g = a - b - c - d + e + f + g;
        println(toString(d)+" "+toString(e)+" "+toString(f) +" " + toString(g)); 
        // a = a >> 0;       
		if(a + b + c + d + e + f + g > 100000000) {
			a = 123;
			b = 456;
			c = 789;
			d = 155;
			e = 123;
			f = 55;
			g = 32;
		}
	}
	println(toString(g));
	return 0;
}

